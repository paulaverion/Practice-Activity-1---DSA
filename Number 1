//Averion

#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
    int arraySize;

    cout << "Enter array size: ";
    cin >> arraySize;

    int elements[arraySize];
    for (int i = 0; i < arraySize; i++);
    {
        int count = 1;
        cout << "Enter element " << count << ": ";
        cin >> elements[i];
        count++;
    }

return 0
}
